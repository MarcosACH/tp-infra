# Archivo de configuración de variables de entorno para el proyecto
# Copia este archivo como .env y completa con tus credenciales reales

# Google Cloud Platform
GCP_PROJECT_ID=usm-infra-grupo2
GCP_BUCKET_NAME=bucket_grupo2_infra
GCP_LOCATION=US

# Ruta al archivo de credenciales JSON de Google Cloud
# Descarga tu archivo de credenciales desde Google Cloud Console
GOOGLE_APPLICATION_CREDENTIALS=./config/google-credentials.json

# Configuración opcional de generación de datos
GENERATION_DAYS=30
DISTRIBUTORS_COUNT=5
CLIENTS_COUNT=10
BRANCHES_COUNT=25
